{
  "name": "awish4me",
  "version": "0.0.1",
  "description": "wish genie",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "installAll": "npm install &&  cd ./backend &&  npm install && cd ../frontend && npm install",
    "backend": "cd backend && pm2 start app.js",
    "server": "pm2 start server.js",
    "start": "concurrently --kill-others-on-fail \"pm2 server\" \"npm backend\"",
    "restart": "pm2 startOrRestart ecosystem.config.js",
    "deploy": "pm2 deploy ecosystem.config.js production",
    "server-start" : "pm2 deploy ecosystem.config.js  production exec 'pm2 start'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suitcasemedia/awish4me.git"
  },
